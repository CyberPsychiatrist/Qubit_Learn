
{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "run_frontend.py"
    }
  ]
}
{
  "buildCommand": "echo 'Frontend is static, no build needed'",
  "outputDirectory": "static",
  "env": {
    "BACKEND_URL": "https://your-production-backend-url.com"
  }
}
